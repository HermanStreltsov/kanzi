{
  "manifest_version": 2,
  "name": "Kanzi",
  "version": "1.4.0",
  "description": "Firefox extension that helps building vocabulary while surfing the internet",

  "applications": {
    "gecko": {
      "id": "kanzi@w3punk.com"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "permissions": ["menus"]
}
